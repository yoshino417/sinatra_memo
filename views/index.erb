  <form class="add-memo" action="add" method="GET">
    <input class="add-button" type="submit" value="追加">
  </form>
  <div class="memo-section">
    <ul class="memo-ul">
      <% unless @memos.nil? %>
      <% @memos.each do |memo| %>
      <li class="memo-li">
        <a href=<%= "/show/#{memo['id']}" %>><%= memo['title'] %></a>
      </li>
      <% end %>
      <% end %>
    </ul>
  </div>
